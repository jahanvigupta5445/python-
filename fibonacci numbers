n=int(input("enter the number of terms you want for fibonacci series:"))
fibonacciList = [0, 1]
# finding 10 terms of the series starting from 3rd term
term = 3
while term < n + 1:
    value = fibonacciList[term - 2] + fibonacciList[term - 3]
    fibonacciList.append(value)
    term = term + 1
print("terms of the fibonacci series are:")
print(fibonacciList)
